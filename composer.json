  "type": "luya-kickstarter",
  "license": "MIT",
  "require": {
    "luyadev/luya-module-admin": "^5.0",
    "luyadev/luya-module-cms": "^5.0",
    "luyadev/luya-bootstrap4": "^1.0",
    "yiisoft/yii2-bootstrap5": "~2.0.2",
    "luyadev/luya-generic": "^1.0",
    "luyadev/luya-module-gallery": "^1.0",
    "siripravi/luya-module-ecommerce": "^1.0.0",
    "siripravi/luya-module-shopcart": "^1.1.0",
    "siripravi/luya-module-forms": "^1.0.0",
    "siripravi/luya-module-authhelper": "^1.0.0",
    "kartik-v/yii2-icons": "~1.4.8",
    "kartik-v/yii2-widgets": "~3.4.1", 
    "kartik-v/yii2-widget-activeform": "~1.6.4",
    "dev7ch/luya-ext-slick": "^1.1",
    "voskobovich/yii2-linker-behavior": "^4.0",
    "yiisoft/yii2-authclient": "~2.2.0",
