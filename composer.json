{
    "name": "luyadev/luya-kickstarter-bootstrap4",
    "description": "LUYA boilerplate to kickstart projects with Bootstrap 4.",
    "type": "luya-kickstarter",
    "license":"MIT",
    "require": {
        "luyadev/luya-module-admin" : "^4.0",
        "luyadev/luya-module-cms" : "^4.0",
        "luyadev/luya-bootstrap4": "^1.0",
        "yiisoft/yii2-bootstrap5": "~2.0.2",
        "luyadev/luya-generic" : "^1.0",        
        "luyadev/luya-module-gallery": "^1.0",
        "siripravi/catalog": "^1.1.0",
        "kartik-v/yii2-icons": "@dev",
        "kartik-v/yii2-widgets": "*",
        "dev7ch/luya-ext-slick": "^1.1",
        "voskobovich/yii2-linker-behavior": "^4.0",
        "arturoliveira/yii2-checkoutprogressbar": "*",
        "sinergi/browser-detector": "^6.1",
        "yiisoft/yii2-authclient": "~2.2.0",
        "chandra/yii2-account":"@dev",
        "yiisoft/yii2-symfonymailer": "~2.0.3",
        "luyadev/luya-module-forms": "^1.8",
        "basepodapps/yii2-feathericons": "^1.0.0"
       
    },
    "require-dev": {
        "yiisoft/yii2-debug" : "^2.0",
        "yiisoft/yii2-gii" : "^2.0",
        "luyadev/luya-deployer": "^1.0",
        "twbs/bootstrap": "^4.0",
        "unglue/client": "^1.5"
    },
    "config": {
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "luyadev/luya-composer": true
        }
    }, 
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "path",
            "url": "./yii2-account"
        }
    ]
}
